echo $((1 + 2))
echo $((1 + 2 * 4 / 2))
echo $((5 - 3 + 8))        
echo $((2 * 3 + 4 / 2))    
echo $((10 % 3))           
echo $((4 ** 2))           
echo $(( (3 + 5) * 2 ))    
echo $((7 > 5))            
echo $((4 < 2))            
echo $((10 == 10))         
echo $((5 != 5))           
echo $((10 / 0))           
